{% extends 'MopaBootstrapSandboxBundle::base.html.twig' %}

{% block title %}Yourapp{% endblock %}

{% block navbar %}
    {% embed '@MopaBootstrap/Navbar/navbar.html.twig' with { fixedTop: true, inverse: true, id: 'navbar-example' } %}
        {% block brand %}
            <a class="navbar-brand" href="{{ path('mopa_primo') }}">Mopa Bootstrap</a>
        {% endblock %}

        {% block menu %}
            {{ mopa_bootstrap_menu('AppBundle:Builder:createMainMenu') }}
            {{ mopa_bootstrap_menu('exampledropdown', {'automenu': 'navbar', 'pull-right': true}) }}
        {% endblock %}
    {% endembed %}
{% endblock navbar %}
